{
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "extends": [
        "standard",
        "plugin:vue/recommended"
    ],
    "plugins": [
        "html",
        "json"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest": true
    }
}
{
  "name": "material-components-vue",
  "version": "0.0.12",
  "description": "material-components-web with vuejs",
  "author": "Mats Pfeiffer",
  "license": "MIT",
  "main": "./src/index.js",
  "scripts": {
    "clean:dist": "rm -rf ./dist/*",
    "clean:output": "rm -rf ./output/*",
    "lint": "eslint --fix ./",
    "build:dev": "yarn run clean:output && NODE_ENV=development && webpack -d --config ./webpack.config.dev.js",
    "build:prod": "yarn run clean:output && NODE_ENV=production && webpack -p --config ./webpack.config.prod.js",
    "build:lib": "yarn run clean:dist && NODE_ENV=production && webpack -p --config ./webpack.config.lib.js",
    "serve:dev": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.dev.js"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "material-components-web": "^0.20.0",
    "material-design-icons": "^3.0.1",
    "vue": "^2.4.2",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.5",
    "cssnano": "^3.10.0",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^3.12.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.7.1"
  }
}
